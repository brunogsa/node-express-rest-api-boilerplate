{
  "parser": "babel-eslint",
  "extends": [
    "standard"
  ],
  "env": {
    "jest": true,
    "jasmine": true
  },
  "rules": {
    "space-before-function-paren": [0, "anonymous"],
    "no-await-in-loop": [2],
    "no-console": [0],
    "block-scoped-var": [2],
    "guard-for-in": [2],
    "no-caller": [2],
    "no-empty-function": [2],
    "no-eq-null": [2],
    "no-eval": [2],
    "no-extra-bind": [2],
    "no-extra-label": [2],
    "no-floating-decimal": [2],
    "no-implicit-coercion": [2],
    "no-implied-eval": [2],
    "no-invalid-this": [2],
    "no-iterator": [2],
    "no-labels": [2],
    "no-loop-func": [2],
    "no-proto": [2],
    "no-return-assign": [2, "always"],
    "no-self-compare": [2],
    "no-sequences": [2],
    "no-throw-literal": [2],
    "no-unmodified-loop-condition": [2],
    "no-unused-expressions": [0],
    "no-useless-call": [2],
    "no-useless-return": [2],
    "no-void": [2],
    "no-with": [2],
    "prefer-promise-reject-errors": [2],
    "radix": [2],
    "yoda": [2],
    "import/no-unresolved": [2],
    "semi": [2, "always"],
    "no-multi-spaces": [0],
    "padded-blocks": [0]
  }
}
